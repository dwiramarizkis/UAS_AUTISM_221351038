    <!-- Question 1 -->
    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        app:cardCornerRadius="12dp"
        app:cardElevation="2dp"
        app:cardBackgroundColor="@color/autism_light_blue"
        app:strokeWidth="1dp"
        app:strokeColor="@color/autism_blue">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="A1: Saya mudah beralih antar aktivitas"
                android:textAppearance="@style/TextAppearance.App.Body1"
                android:textColor="@color/autism_blue"
                android:textStyle="bold"
                android:layout_marginBottom="12dp"/>

            <RadioGroup
                android:id="@+id/rg_a1"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.google.android.material.radiobutton.MaterialRadioButton
                    android:id="@+id/rb_a1_yes"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Ya"
                    android:textAppearance="@style/TextAppearance.App.Body2"
                    android:buttonTint="@color/autism_blue"
                    android:layout_marginEnd="16dp"/>

                <com.google.android.material.radiobutton.MaterialRadioButton
                    android:id="@+id/rb_a1_no"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Tidak"
                    android:textAppearance="@style/TextAppearance.App.Body2"
                    android:buttonTint="@color/autism_blue"/>
            </RadioGroup>
        </LinearLayout>
    </com.google.android.material.card.MaterialCardView>

